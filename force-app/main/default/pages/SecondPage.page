<apex:page standardController="Account">
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockButtons >
                <apex:commandButton value="Save" title="Save" action="{!save}" />
                <apex:commandButton value="qSave" title="qSave" action="{!quicksave}" />
                <apex:commandButton value="Cancel" title="Cancel" action="{!Cancel}" />
            </apex:pageBlockButtons>
            <apex:pageBlockSection title="Acc info">
                <apex:inputField value="{!Account.Name}" />
                <apex:inputField value="{!Account.Type}" /> 
                <apex:inputField value="{!Account.Rating}" />
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>